<ResourceDictionary x:Class="QPlayer.ThemesV2.Icons" x:ClassModifier="public"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:svgc="http://sharpvectors.codeplex.com/svgc/">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../Resources/Icons/ConvertedIcons.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    <ImageSource x:Key="IconImage">../Resources/Icon.ico</ImageSource>

    <DrawingImage x:Key="IconBlank"/>
    <DrawingImage x:Key="IconPlay" Drawing="{StaticResource Icon_play_solidDrawingGroup}"/>
    <DrawingImage x:Key="IconPause" Drawing="{StaticResource Icon_pause_solidDrawingGroup}"/>
    <DrawingImage x:Key="IconStop" Drawing="{StaticResource Icon_stop_solidDrawingGroup}"/>

    <DrawingImage x:Key="IconGroupCue" Drawing="{StaticResource Icon_folder_solidDrawingGroup}"/>
    <DrawingImage x:Key="IconStopCue" Drawing="{StaticResource Icon_stop_solidDrawingGroup}"/>
    <DrawingImage x:Key="IconDummyCue" Drawing="{StaticResource Icon_circle_solidDrawingGroup}"/>
    <DrawingImage x:Key="IconSoundCue" Drawing="{StaticResource Icon_music_solidDrawingGroup}"/>
    <DrawingImage x:Key="IconTimeCodeCue" Drawing="{StaticResource Icon_clock_solidDrawingGroup}"/>
    <DrawingImage x:Key="IconVolumeCue" Drawing="{StaticResource Icon_volume_low_solidDrawingGroup}"/>
    <DrawingImage x:Key="IconVideoCue" Drawing="{StaticResource Icon_film_solidDrawingGroup}"/>
    <DrawingImage x:Key="IconVideoFramingCue" Drawing="{StaticResource Icon_crop_simple_solidDrawingGroup}"/>
    <DrawingImage x:Key="IconShaderParamsCue" Drawing="{StaticResource Icon_sliders_solidDrawingGroup}"/>

    <DrawingImage x:Key="IconOneShot" Drawing="{StaticResource Icon_arrow_right_long_solidDrawingGroup}"/>
    <DrawingImage x:Key="IconLoop" Drawing="{StaticResource Icon_rotate_solidDrawingGroup}"/>
    <DrawingImage x:Key="IconHoldLast" Drawing="{StaticResource Icon_arrow_right_to_bracket_solidDrawingGroup}"/>

    <DrawingImage x:Key="IconWithLast" Drawing="{StaticResource Icon_arrow_turn_left_solid_fullDrawingGroup}"/>
    <DrawingImage x:Key="IconAfterLast" Drawing="{StaticResource Icon_arrow_turn_left_wait_solid_fullDrawingGroup}"/>
</ResourceDictionary>